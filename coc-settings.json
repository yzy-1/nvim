{
	// preferences
	"coc.preferences.enableFloatHighlight": true,
	"coc.preferences.extensionUpdateCheck": "daily",
	"coc.preferences.messageLevel": "error",
	// diagnostic
	"diagnostic.infoSign": "I>",
	"diagnostic.hintSign": "H>",
	"diagnostic.warningSign": "W>",
	"diagnostic.errorSign": "E>",
	"diagnostic.enable": true,
	"diagnostic.highlightOffset": 1,
	"diagnostic.checkCurrentLine": true,
	"diagnostic.virtualText": true,
	"diagnostic.virtualTextPrefix": " ❯❯❯ ",
	// source
	"coc.source.around.enable": false,
	"coc.source.buffer.enable": false,
	"yank.enableCompletion": false,
	// suggest
	"suggest.completionItemKindLabels": {
		"keyword": "\uf1de",
		"variable": "\ue79b",
		"value": "\uf89f",
		"operator": "\u03a8",
		"function": "\u0192",
		"reference": "\ufa46",
		"constant": "\uf8fe",
		"method": "\uf09a",
		"struct": "\ufb44",
		"class": "\uf0e8",
		"interface": "\uf417",
		"text": "\ue612",
		"enum": "\uf435",
		"enumMember": "\uf02b",
		"module": "\uf40d",
		"color": "\ue22b",
		"property": "\ue624",
		"field": "\uf9be",
		"unit": "\uf475",
		"event": "\ufacd",
		"file": "\uf723",
		"folder": "\uf114",
		"snippet": "\ue60b",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
	"suggest.snippetIndicator": "►",
	"suggest.disableMenuShortcut": true,
	"suggest.disableMenu": true,
	"suggest.detailMaxLength": 60,
	"suggest.enablePreview": true,
	"suggest.floatEnable": true,
	"suggest.detailField": "preview",
	"suggest.noselect": false,
	"suggest.enablePreselect": false,
	"suggest.triggerAfterInsertEnter": true,
	"suggest.autoTrigger": "always",
	"suggest.timeout": 5000,
	"suggest.triggerCompletionWait": 100,
	// format
	"coc.preferences.formatOnSaveFiletypes": [
		"cpp",
		"c",
		"python",
		"go",
		"json"
	],
	// javascript
	"json.format.enable": true,
	// vimlsp
	"vimlsp.indexes.runtimepath": true,
	"vimlsp.indexes.gap": 10,
	"vimlsp.indexes.count": 3,
	"vimlsp.suggest.fromVimruntime": true,
	"vimlsp.suggest.fromRuntimepath": false,
	// pairs
	"pairs.enableCharacters": [
		"(",
		"[",
		"{",
		"'",
		"\""
	],
	// languageserver
	"languageserver": {}
}
